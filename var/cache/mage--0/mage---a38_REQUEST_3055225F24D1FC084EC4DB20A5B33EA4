<script type="text/javascript">
	jQuery(function() {
		jQuery( "#slider-range" ).slider({
			range: true,
			step: 100,
			min: 600,
			max: 1200,
			values: [ 600, 1200 ],
			slide: function( event, ui ) {
				jQuery( "#nav-slider-price-from" ).val( ui.values[ 0 ]);
				jQuery( "#nav-slider-price-to" ).val( ui.values[ 1 ]);
								jQuery( "#nav-decimal-from-price-tooltip").html(ui.values[ 0 ] );
				var pos = jQuery( "#slider-range .af-first-handle" ).position();
				jQuery("#nav-decimal-from-price-tooltip").css({"left": pos.left - 10}).show();
				jQuery( "#nav-decimal-to-price-tooltip").html( ui.values[ 1 ]);
				var pos = jQuery( "#slider-range .af-last-handle" ).position();
				jQuery("#nav-decimal-to-price-tooltip").css({"left": pos.left - 10}).show();
							},
			create: function(event, ui) { 
				jQuery("#slider-range a.ui-slider-handle:first").addClass("af-first-handle");
				jQuery("#slider-range a.ui-slider-handle:last").addClass("af-last-handle");
								jQuery("#slider-range").append("<div class='slider-value no-display' id='nav-decimal-from-price-tooltip'></div><div class='slider-value no-display' id='nav-decimal-to-price-tooltip'></div>");
                	
				}
		});	

		jQuery("#nav-slider-price-from").change( function(){
			var sl_min = jQuery("#nav-slider-price-from").val();
			var sl_max = jQuery("#nav-slider-price-to").val();
			jQuery( "#slider-range" ).slider( "option", "values", [sl_min,sl_max] );
			setChangePrice();
			var pos = jQuery(this).position();
			getFilterData(pos.top, pos.left);
		});
		jQuery("#nav-slider-price-to").change( function(){
			var sl_min = jQuery("#nav-slider-price-from").val();
			var sl_max = jQuery("#nav-slider-price-to").val();
			jQuery( "#slider-range" ).slider( "option", "values", [sl_min,sl_max] );
			setChangePrice();
			var pos = jQuery(this).position();
			getFilterData(pos.top, pos.left);
		});	
		jQuery( "#slider-range" ).bind( "slidestop", function(event, ui) {
			setChangePrice();
			var pos = jQuery(this).position();
			getFilterData(pos.top, pos.left);
			});	
				jQuery( "#slider-range" ).bind( "mouseenter", function(event, ui) {
			if(jQuery( "#nav-slider-price-from").val().length)
			{
				var pos = jQuery( "#slider-range .af-first-handle" ).position();
				jQuery("#nav-decimal-from-price-tooltip").css({"left": pos.left - 10});
				var pos = jQuery( "#slider-range .af-last-handle" ).position();
				jQuery("#nav-decimal-to-price-tooltip").css({"left": pos.left - 10});
				jQuery("#nav-decimal-from-price-tooltip").show();
			}
			if(jQuery( "#nav-slider-price-to").val().length)
			{
				var pos = jQuery( "#slider-range .af-first-handle" ).position();
				jQuery("#nav-decimal-from-price-tooltip").css({"left": pos.left - 10});
				var pos = jQuery( "#slider-range .af-last-handle" ).position();
				jQuery("#nav-decimal-to-price-tooltip").css({"left": pos.left - 10});
				jQuery("#nav-decimal-to-price-tooltip").show();
			}			
			});	
		jQuery( "#slider-range" ).bind( "mousemove", function(event, ui) {
			if(jQuery( "#nav-slider-price-from").val().length)
			{
				var pos = jQuery( "#slider-range .af-first-handle" ).position();
				jQuery("#nav-decimal-from-price-tooltip").css({"left": pos.left - 10});
				var pos = jQuery( "#slider-range .af-last-handle" ).position();
				jQuery("#nav-decimal-to-price-tooltip").css({"left": pos.left - 10});
				jQuery("#nav-decimal-from-price-tooltip").show();
			}
			if(jQuery( "#nav-slider-price-to").val().length)
			{
				var pos = jQuery( "#slider-range .af-first-handle" ).position();
				jQuery("#nav-decimal-from-price-tooltip").css({"left": pos.left - 10});
				var pos = jQuery( "#slider-range> .af-last-handle" ).position();
				jQuery("#nav-decimal-to-price-tooltip").css({"left": pos.left - 10});
				jQuery("#nav-decimal-to-price-tooltip").show();
			}			
			});
		jQuery( "#slider-range" ).bind( "mouseleave", function(event, ui) {
			if(jQuery( "#nav-slider-price-from").val().length)
			{
				jQuery("#nav-decimal-from-price-tooltip").hide();
			}
			if(jQuery( "#nav-slider-price-to").val().length)
			{
				jQuery("#nav-decimal-to-price-tooltip").hide();
			}			
			});	
		jQuery(function (){
			jQuery( "#nav-decimal-from-price-tooltip").html(jQuery( "#nav-slider-price-from").val());
			jQuery( "#nav-decimal-to-price-tooltip").html(jQuery( "#nav-slider-price-to").val());
			});
						
	});
</script>
